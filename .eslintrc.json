{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": "off",
        "prefer-destructuring": ["error", {"object": true, "array": false}]
    }
}
